{
	"name": "APIGET",
	"properties": {
		"activities": [
			{
				"name": "GETUSERSAPI",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"headers": {
						"x-api-key": "reqres-free-v1"
					},
					"url": "https://reqres.in/api/users?page=1"
				}
			},
			{
				"name": "ForEachUser",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "GETUSERSAPI",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@range(1,activity('GETUSERSAPI').output.total_pages)",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy data1",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET",
									"additionalHeaders": {
										"x-api-key": "reqres-free-v1"
									},
									"paginationRules": {
										"supportRFC5988": "true"
									}
								},
								"sink": {
									"type": "AzureSqlSink",
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"path": "$['page']"
											}
										},
										{
											"source": {
												"path": "$['per_page']"
											}
										},
										{
											"source": {
												"path": "$['total']"
											}
										},
										{
											"source": {
												"path": "$['total_pages']"
											}
										},
										{
											"source": {
												"path": "$['data'][0]['id']"
											}
										},
										{
											"source": {
												"path": "$['data'][0]['email']"
											}
										},
										{
											"source": {
												"path": "$['data'][0]['first_name']"
											}
										},
										{
											"source": {
												"path": "$['data'][0]['last_name']"
											}
										},
										{
											"source": {
												"path": "$['data'][0]['avatar']"
											}
										},
										{
											"source": {
												"path": "$['support']['url']"
											}
										},
										{
											"source": {
												"path": "$['support']['text']"
											}
										}
									]
								}
							},
							"inputs": [
								{
									"referenceName": "RestGetUsers",
									"type": "DatasetReference",
									"parameters": {
										"PageNo": "?page=1"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "SQLUsers",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"annotations": []
	}
